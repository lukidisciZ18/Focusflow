{
    "name": "FocusFlow - Personal Focus Coach",
    "short_name": "FocusFlow",
    "description": "Build sustainable focus habits with personalized coaching, smart reminders, and culturally diverse wisdom.",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#10B981",
    "theme_color": "#10B981",
    "orientation": "portrait-primary",
    "scope": "/",
    "lang": "en",
    "categories": ["productivity", "health", "education", "lifestyle"],
    "prefer_related_applications": false,
    "edge_side_panel": {
        "preferred_width": 400
    },
    "launch_handler": {
        "client_mode": "focus-existing"
    },
    "protocol_handlers": [
        {
            "protocol": "web+focusflow",
            "url": "/?action=%s"
        }
    ],
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "text/plain": [".txt"],
                "application/json": [".json"]
            }
        }
    ],
    "icons": [
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "192x192",
            "type": "image/svg+xml",
            "purpose": "any maskable"
        },
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "512x512",
            "type": "image/svg+xml",
            "purpose": "any maskable"
        },
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "72x72",
            "type": "image/svg+xml",
            "purpose": "any"
        },
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "96x96",
            "type": "image/svg+xml",
            "purpose": "any"
        },
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "144x144",
            "type": "image/svg+xml",
            "purpose": "any"
        },
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "168x168",
            "type": "image/svg+xml",
            "purpose": "any"
        }
    ],
    "shortcuts": [
        {
            "name": "Quick Focus",
            "short_name": "Quick Focus",
            "description": "Start a 25-minute focus session",
            "url": "/?session=quick",
            "icons": [
                {
                    "src": "data:image/svg+xml;base64,",
                    "sizes": "96x96",
                    "type": "image/svg+xml"
                }
            ]
        },
        {
            "name": "Deep Work",
            "short_name": "Deep Work",
            "description": "Start a 90-minute deep work session",
            "url": "/?session=deep",
            "icons": [
                {
                    "src": "data:image/svg+xml;base64,",
                    "sizes": "96x96",
                    "type": "image/svg+xml"
                }
            ]
        },
        {
            "name": "Marathon Session",
            "short_name": "Marathon",
            "description": "Start a 3+ hour marathon session",
            "url": "/?session=marathon",
            "icons": [
                {
                    "src": "data:image/svg+xml;base64,",
                    "sizes": "96x96",
                    "type": "image/svg+xml"
                }
            ]
        },
        {
            "name": "Analytics",
            "short_name": "Analytics",
            "description": "View your focus analytics and progress",
            "url": "/?view=analytics",
            "icons": [
                {
                    "src": "data:image/svg+xml;base64,",
                    "sizes": "96x96",
                    "type": "image/svg+xml"
                }
            ]
        }
    ],
    "screenshots": [
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "1280x720",
            "type": "image/svg+xml",
            "form_factor": "wide"
        },
        {
            "src": "data:image/svg+xml;base64,",
            "sizes": "375x812",
            "type": "image/svg+xml",
            "form_factor": "narrow"
        }
    ],
    "related_applications": [],
    "share_target": {
        "action": "/",
        "method": "GET",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "handle_links": "preferred",
    "capture_links": "existing-client-navigate",
    "permissions": [
        "notifications",
        "background-sync",
        "periodic-background-sync"
    ],
    "background": {
        "service_worker": "/sw.js",
        "type": "module"
    },
    "display_override": ["standalone", "minimal-ui"],
    "iarc_rating_id": "",
    "scope_extensions": [],
    "id": "focusflow-app"
} 